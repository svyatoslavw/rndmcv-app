{
  "name": "rndmcv",
  "private": true,
  "version": "1.0.1",
  "license": "ISC",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky",
    "docker:up": "docker-compose -f ./docker-compose.yml up -d",
    "docker:down": "docker-compose -f ./docker-compose.yml down",
    "release": "pnpm version minor && conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "devDependencies": {
    "@rndm/eslint": "workspace:*",
    "@rndm/prettier": "workspace:*",
    "@rndm/typescript": "workspace:*",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "conventional-changelog-cli": "^5.0.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "turbo": "^2.3.0",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=20"
  }
}
